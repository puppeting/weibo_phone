<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:layout_width="match_parent"
        android:gravity="center"
        android:layout_height="match_parent">


        <com.flyco.roundview.RoundLinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
             android:background="@color/bg_color"
            app:rv_backgroundColor="@color/bg_color"
            app:rv_cornerRadius="6dp"
            app:rv_strokeColor="@color/bg_color"
            app:rv_strokeWidth="1dp">

            <LinearLayout
                android:layout_width="330dp"
                android:layout_height="275dp"

                android:orientation="vertical"
                android:padding="@dimen/dimens10">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_title"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:paddingBottom="@dimen/dimens10"
                        android:paddingTop="5dp"
                        android:text="拉流设置"
                        android:textColor="@color/color_text_title"
                        android:textSize="12sp"
                        tools:ignore="MissingConstraints" />

                </LinearLayout>
                <View
                    android:id="@+id/view3"
                    android:layout_width="match_parent"
                    android:layout_height="2px"
                    android:background="@color/color_ddd" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimens10"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="@dimen/dimen60dp"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/dimens10"
                        android:text="地址1"
                        android:textColor="@color/color_333"
                        android:textSize="12sp"
                        tools:ignore="MissingConstraints" />

                    <com.flyco.roundview.RoundLinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        app:rv_backgroundColor="@color/white"
                        app:rv_cornerRadius="5dp"
                        app:rv_strokeColor="@color/white"
                        app:rv_strokeWidth="1dp">

                        <androidx.appcompat.widget.AppCompatEditText
                            android:layout_width="match_parent"
                            android:layout_height="32dp"
                            android:background="@null"
                            android:hint=""
                            android:padding="@dimen/dp_4"
                            android:textColorHint="#868686"
                            android:textSize="@dimen/sp_12" />
                    </com.flyco.roundview.RoundLinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimens10"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimen60dp"
                        android:layout_weight="1"
                        android:text="或推流到本机位 rtsp://192.168.1.123"
                        android:textColor="@color/color_666"
                        android:textSize="10sp"
                        tools:ignore="MissingConstraints" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimens10"
                        android:background="@drawable/shape_dizhi"
                        android:paddingBottom="@dimen/dimen6dp"
                        android:paddingLeft="@dimen/dp_8"
                        android:paddingRight="@dimen/dp_8"
                        android:paddingTop="@dimen/dimen6dp"
                        android:text="复制地址"
                        android:textColor="#3241A2"
                        android:textSize="10sp"
                        tools:ignore="MissingConstraints" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimens10"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="@dimen/dimen60dp"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/dimens10"
                        android:text="地址2"
                        android:textColor="@color/color_333"
                        android:textSize="12sp"
                        tools:ignore="MissingConstraints" />

                    <com.flyco.roundview.RoundLinearLayout

                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        app:rv_backgroundColor="@color/white"
                        app:rv_cornerRadius="5dp"
                        app:rv_strokeColor="@color/white"
                        app:rv_strokeWidth="1dp">

                        <androidx.appcompat.widget.AppCompatEditText
                            android:id="@+id/et_search"
                            android:layout_width="match_parent"
                            android:layout_height="32dp"
                            android:background="@null"
                            android:hint=""
                            android:padding="@dimen/dp_4"
                            android:textColorHint="#868686"
                            android:textSize="@dimen/sp_12" />
                    </com.flyco.roundview.RoundLinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimens10"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="60dp"
                        android:layout_weight="1"
                        android:text="或推流到本机位 rtsp://192.168.1.123"
                        android:textColor="@color/color_666"
                        android:textSize="10sp"
                        tools:ignore="MissingConstraints" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimens10"
                        android:background="@drawable/shape_dizhi"
                        android:paddingBottom="@dimen/dimen6dp"
                        android:paddingLeft="@dimen/dp_8"
                        android:paddingRight="@dimen/dp_8"
                        android:paddingTop="@dimen/dimen6dp"
                        android:text="复制地址"
                        android:textColor="#3241A2"
                        android:textSize="10sp"
                        tools:ignore="MissingConstraints" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearLayout2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_15"
                    android:gravity="center"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent">


                    <TextView
                        android:id="@+id/tv_ok"
                        android:layout_width="140dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center"
                        android:background="@drawable/shape_login"
                        android:gravity="center"
                        android:text="确定"
                        android:textColor="@color/white"
                        android:textSize="12sp" />
                </LinearLayout>



            </LinearLayout>

        </com.flyco.roundview.RoundLinearLayout>


    </LinearLayout>

    <data>

    </data>
</layout>